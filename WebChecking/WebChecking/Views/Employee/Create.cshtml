@model WebChecking.ViewModel.EmployeeDepartmentViewModel
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>
@using (Html.BeginForm("Create","Employee"))
{
    @Html.Label("Employee Name")
    @Html.TextBoxFor(c=>c.Employee.EName)

    @Html.Label("Department")
    @Html.DropDownListFor(m=>m.Departments,new SelectList(Model.Departments,"Id","DName")," ")

    <input type="submit" value="Submit" />
}
